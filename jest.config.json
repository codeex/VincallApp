{
  "testTimeout": 15000,
  "clearMocks": true,
  "coverageDirectory": "coverage",
  "setupFilesAfterEnv": ["<rootDir>/test/units/setupTests.ts"],
  "transform": {
    "^.+\\.(js|jsx|ts|tsx)$": "ts-jest"
  },
  "testMatch": ["<rootDir>/test/**/*.test.ts?(x)"],
  "cacheDirectory": "./.cache",
  "moduleNameMapper": {
    "^.+\\.css$": "identity-obj-proxy",
    "^src/(.*)$": "<rootDir>/src/$1"
  },
  "globals": {
    "window": {
    }
  }
}